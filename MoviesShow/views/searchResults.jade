                    tr
                      th No.
                      th Movie Name
                      th Năm SX
                      th Chất lượng
                      th Quốc gia
                      th Thể loại
                      th Rating
                  tbody
                   each movie, i in searchList
                    tr
                      td #{i+1}
                      td.w3-list-img
                        a(href='/movieinfo?id=#{movie.id}')
                          img(src='#{movie.srcImg}', alt='', style="width:auto;height:auto;")
                          span (id:#{movie.id})  #{movie.name1}
                          span (id:#{movie.id})  #{movie.name2}
                      td 2011
                      td HD
                      td.w3-list-info
                       each country, j in movie.country
                        li
                            a(href="/quoc-gia?country=#{country}") #{country}
                            
                      td.w3-list-info(style="white-space: normal;")
                       each genre, j in movie.genres
                        li
                            a(href="/the-loai?genre=#{genre}") #{genre}
                        br
                      td #{movie.imdb}
