    // header
    .header
      .container
        .w3layouts_logo
          a(href='/')
            h1
              | One
              span Movies
        .w3_search(onmouseleave='hiddenTable()')
          form(action='/searchResults', method='post')
            script(src='js/searchBox2.js')
            script.
                function hiddenTable(){
                document.getElementById("myTable").style.display="none";
                }
                function showTable(){
                document.getElementById("myTable").style.display="block";
                LogData()
                }
            input#myInput(type='text', name='search', onkeyup='LogData()', placeholder='Search', required='' onclick="showTable()")
            input(type='submit', value='Go')
            table#myTable
              tbody
                tr.header
